<Window x:Class="CHashWPFEditor.SearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CHashWPFEditor"
        mc:Ignorable="d"
        Title="Search and Replace" Height="144.123" Width="297">
    <StackPanel Margin="0,0,0,1">
        <TextBox x:Name="textSearch" Height="23" TextWrapping="Wrap" Text="Search" Margin="0,10,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100"/>
        <TextBox x:Name="textReplace" Height="23" TextWrapping="Wrap" Text="Replace" Margin="0,10,0,0" HorizontalAlignment="Center" Width="100"/>
        <Button x:Name="button" Content="Zastąp" Margin="0,10,0,0" Click="button_Click" HorizontalAlignment="Center" VerticalAlignment="Center" Width="70"/>
    </StackPanel>
</Window>
